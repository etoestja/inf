#! /usr/bin/gnuplot -persist
set terminal postscript eps enhanced
set output "./plot/15.ps"
set encoding utf8
set xlabel "I^{1/2}, cu" font "NimbusSanL-Regu,18"
set nokey
#set bmargin 4
set ylabel "V_1, V" font "NimbusSanL-Regu,18"
#set logscale x
set grid
#set xrange [2200:3000]
set yrange [-3:5]
set mxtics 10

set title "Lab 1.1: Line 15, {/Symbol l} = 6143.06 Ã…"
#set style line 1 lt 1 pt 7 ps 0.5

f(x)=a + (b * x)
fit f(x) "<(sed -n '1,5p' data15.csv)" using 1:2 via a,b

plot "data15.csv" using 1:2:3:4 with xyerrorbars, f(x)
