CFLAGS=-m32
all: controller starter
controller: multicast.o controller.o common.o
	gcc $(CFLAGS) multicast.o common.o controller.o -o $@
starter: multicast.o starter.o common.o
	gcc $(CFLAGS) multicast.o common.o starter.o -o $@
.o: common.h $<.c common.c
	gcc $(CFLAGS) $<.c -o $@
clean:
	rm -rf *.o
	rm -rf starter controller
	rm -rf *~
